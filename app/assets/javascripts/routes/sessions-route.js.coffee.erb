angular.module('session')
  .config ($routeProvider) ->
    $routeProvider
      .when '/login',
        templateUrl: '<%= asset_path("/assets/sessions/new.html") %>'
        controller: 'SessionsNewController'

      .when '/logout',
        templateUrl: '<%= asset_path("/assets/sessions/destroy.html") %>'
        controller: 'SessionsDestroyController'
